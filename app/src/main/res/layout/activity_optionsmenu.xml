<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?attr/bckImg"
    tools:context=".OptionsMenu">

    <Button
        android:id="@+id/rturnMenu"
        android:layout_width="207dp"
        android:layout_height="58dp"
        android:layout_marginStart="102dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="102dp"
        android:layout_marginBottom="23dp"
        android:background="@drawable/rounded_button"
        android:foregroundGravity="center_horizontal"
        android:gravity="center"
        android:text="BACK"
        android:textColor="?attr/tC"
        app:layout_constraintBottom_toTopOf="@+id/textView11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView17" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="238dp"
        android:layout_height="42dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="4dp"
        android:gravity="center|left"
        android:text="Cloud Storage Backup"
        android:textColor="?attr/tC"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/switch4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        tools:ignore="RtlHardcoded" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="238dp"
        android:layout_height="42dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="2dp"
        android:gravity="center|left"
        android:text="Language"
        android:textColor="?attr/tC"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/textView15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/themeTextView4"
        tools:ignore="RtlHardcoded" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="94dp"
        android:layout_height="42dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="37dp"
        android:gravity="center"
        android:text="Default"
        android:textColor="?attr/tC"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView16"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="238dp"
        android:layout_height="42dp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="2dp"
        android:gravity="center|left"
        android:text="UI Scaling"
        android:textColor="?attr/tC"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/textView17"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView14"
        tools:ignore="RtlHardcoded" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="94dp"
        android:layout_height="42dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="37dp"
        android:gravity="center"
        android:text="English"
        android:textColor="?attr/tC"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/themeTextView4" />

    <Switch
        android:id="@+id/switch4"
        android:layout_width="91dp"
        android:layout_height="48dp"
        android:layout_marginEnd="42dp"
        android:checked="true"
        android:gravity="center|center_horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/switch3"
        tools:ignore="UseSwitchCompatOrMaterialXml" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="238dp"
        android:layout_height="42dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="6dp"
        android:gravity="center|left"
        android:text="Debugging Mode"
        android:textColor="?attr/tC"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/switch3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        tools:ignore="RtlHardcoded" />

    <Switch
        android:id="@+id/switch3"
        android:layout_width="91dp"
        android:layout_height="48dp"
        android:layout_marginEnd="42dp"
        android:checked="true"
        android:gravity="center|center_horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/switch2"
        tools:ignore="UseSwitchCompatOrMaterialXml" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="238dp"
        android:layout_height="42dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="6dp"
        android:filterTouchesWhenObscured="true"
        android:gravity="center|left"
        android:text="Online Visibility"
        android:textColor="?attr/tC"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/switch2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        tools:ignore="RtlHardcoded" />

    <Switch
        android:id="@+id/switch2"
        android:layout_width="91dp"
        android:layout_height="48dp"
        android:layout_marginEnd="42dp"
        android:gravity="center|center_horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/switch1"
        tools:ignore="UseSwitchCompatOrMaterialXml" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="238dp"
        android:layout_height="42dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="24dp"
        android:gravity="center|left"
        android:text="Share analytics"
        android:textColor="?attr/tC"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/switch1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/themeTextView"
        tools:ignore="RtlHardcoded" />

    <TextView
        android:id="@+id/themeTextView"
        android:layout_width="335dp"
        android:layout_height="48dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="36dp"
        android:text="Options"
        android:textColor="?attr/tC"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/themeTextView4"
        android:layout_width="335dp"
        android:layout_height="48dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="36dp"
        android:text="Accessibility"
        android:textColor="?attr/tC"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/switch4" />

    <Switch
        android:id="@+id/switch1"
        android:layout_width="91dp"
        android:layout_height="48dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="42dp"
        android:checked="true"
        android:gravity="center|center_horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/themeTextView"
        tools:ignore="UseSwitchCompatOrMaterialXml" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="335dp"
        android:layout_height="32dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="38dp"
        android:gravity="center"
        android:text="Gollect V0.0.25 - 21/06/2022"
        android:textColor="?attr/tC"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rturnMenu" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="351dp"
        android:layout_height="70dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="36dp"
        android:textColor="?attr/tC"
        android:gravity="center"
        android:text="Gollect is a simple, multi-faceted, fast and responsive collection application using Google Firebase. Application subject to change."
        android:textAlignment="viewStart"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="RtlCompat" />
</androidx.constraintlayout.widget.ConstraintLayout>